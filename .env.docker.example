# =============================================================================
# xWin_Dash - Docker Environment Variables
# =============================================================================
# Copy this file to .env and fill in the values before running docker-compose
# NEVER commit the .env file to version control!
# =============================================================================

# =============================================================================
# DATABASE CONFIGURATION (REQUIRED)
# =============================================================================
DB_DATABASE=xwin_dash
DB_USERNAME=postgres
DB_PASSWORD=your_secure_password_here

# =============================================================================
# API KEYS (Optional - for AI features)
# =============================================================================
OPENAI_API_KEY=sk-your-openai-api-key
GOOGLE_API_KEY=your-google-api-key

# =============================================================================
# APPLICATION SETTINGS
# =============================================================================
APP_ENV=production
APP_DEBUG=false
APP_KEY=base64:generate-a-new-app-key-with-php-artisan-key-generate
