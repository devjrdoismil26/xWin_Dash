{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "abraham/twitteroauth": "^7.0",
        "amirsarhang/instagram-php-sdk": "^3.0",
        "facebook/php-business-sdk": "^18.0",
        "google-gemini-php/client": "^1.0",
        "google/apiclient": "^2.15",
        "googleads/google-ads-php": "^29.0",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2.7",
        "happyr/linkedin-api-client": "^0.2.2",
        "inertiajs/inertia-laravel": "^1.0",
        "intervention/image": "^3.0",
        "jstolpe/tiktok-api-php-sdk": "^1.0",
        "laravel-workflow/laravel-workflow": "^1.0",
        "laravel/framework": "^10.10",
        "laravel/horizon": "^5.0",
        "laravel/sanctum": "^3.3",
        "laravel/socialite": "^5.23",
        "laravel/tinker": "^2.8",
        "league/csv": "^9.24",
        "maatwebsite/excel": "^3.1",
        "netflie/whatsapp-cloud-api": "^2.2",
        "noweh/twitter-api-v2-php": "^3.7",
        "php-http/guzzle7-adapter": "^1.1",
        "php-http/message": "^1.16",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-image-optimizer": "^1.8",
        "spatie/laravel-medialibrary": "^11.0",
        "spatie/laravel-permission": "^5.0",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "*",
        "larastan/larastan": "*",
        "laravel/breeze": "^1.29",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpmetrics/phpmetrics": "^2.9",
        "phpstan/phpstan": "*",
        "phpstan/phpstan-doctrine": "^1.5",
        "phpstan/phpstan-nette": "^1.3",
        "phpstan/phpstan-phpunit": "^1.4",
        "phpstan/phpstan-symfony": "^1.4",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.0",
        "squizlabs/php_codesniffer": "^3.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Domains\\": "app/Domains/",
            "App\\Shared\\": "app/Shared/",
            "Database\\Seeders\\": "database/seeders/",
            
            "App\\Domains\\Core\\Models\\": "app/Domains/Core/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\Dashboard\\Models\\": "app/Domains/Dashboard/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\SocialBuffer\\Models\\": "app/Domains/SocialBuffer/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\Aura\\Models\\": "app/Domains/Aura/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\NodeRed\\Models\\": "app/Domains/NodeRed/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\AI\\Models\\": "app/Domains/AI/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\Projects\\Models\\": "app/Domains/Projects/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\Integrations\\Models\\": "app/Domains/Integrations/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\ADStool\\Models\\": "app/Domains/ADStool/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\EmailMarketing\\Models\\": "app/Domains/EmailMarketing/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\Leads\\Models\\": "app/Domains/Leads/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\Products\\Models\\": "app/Domains/Products/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\Users\\Models\\": "app/Domains/Users/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\Workflows\\Models\\": "app/Domains/Workflows/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\Categorization\\Models\\": "app/Domains/Categorization/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\Activity\\Models\\": "app/Domains/Activity/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\Analytics\\Models\\": "app/Domains/Analytics/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\Auth\\Models\\": "app/Domains/Auth/Infrastructure/Persistence/Eloquent/",
            "App\\Domains\\Media\\Models\\": "app/Domains/Media/Infrastructure/Persistence/Eloquent/"
        },
        "classmap": [
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
