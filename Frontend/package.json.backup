{
  "name": "xwin-dash-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-types": "tsc && vite build",
    "type-check": "NODE_OPTIONS=--max-old-space-size=4096 tsc --noEmit",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "test:e2e": "cypress run",
    "test:e2e:puppeteer": "node scripts/run-e2e-puppeteer.js",
    "test:e2e:users": "node src/modules/Users/tests/e2e/run-users-e2e.js",
    "test:all": "npm run test:run && npm run test:e2e:puppeteer",
    "test:users": "npm run test:e2e:users",
    "test:users:isolated": "node src/modules/Users/tests/e2e/run-all-isolated-tests.js",
    "test:unit": "vitest run",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:canvas": "cypress run --spec 'cypress/e2e/modules/*canvas*.cy.ts'",
    "test:visual": "cypress run --spec 'cypress/e2e/modules/*management*.cy.ts'",
    "test:layouts": "cypress run --spec 'cypress/e2e/layouts/*.cy.ts'",
    "refactor:any": "ts-node ../scripts/refactor-any-simple.ts",
    "generate:jsdoc": "ts-node ../scripts/generate-jsdoc.ts",
    "refactor:all": "ts-node ../scripts/run-all.ts"
  },
